{
  "name": "uniapp-cli",
  "version": "0.1.0-dev.4",
  "description": "一套用于 WEB APP 开发 和 Mobile APP 开发的解决方案",
  "main": "./cli.js",
  "typings": "./cli.d.ts",
  "scripts": {
    "start": "tsc --watch",
    "lint": "tslint src/**/*.ts -c tslint.json",
    "build": "npm run lint && tsc --declaration",
    "link": "npm run build && ./scripts/npm-link.sh",
    "unlink": "cd dist && npm unlink",
    "deploy": "./scripts/deploy.sh",
    "prepublishOnly": "exit 1",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "uniapp": "./bin/uniapp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maolion/uniapp-cli.git"
  },
  "keywords": [
    "uniapp",
    "uniapp-cli",
    "spa",
    "react",
    "react native",
    "web",
    "webpack"
  ],
  "author": "maolion.j@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maolion/uniapp-cli/issues"
  },
  "homepage": "https://github.com/maolion/uniapp-cli#readme",
  "devDependencies": {
    "@types/chalk": "^0.4.31",
    "@types/fs-extra": "^2.0.0",
    "@types/inquirer": "0.0.32",
    "@types/node": "^7.0.12",
    "tslint": "^5.0.0",
    "typescript": "^2.2.2",
    "vts": "^5.0.1"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "cli-spinner": "^0.2.6",
    "clime": "^0.5.0-dev.3",
    "fs-extra": "^2.1.2",
    "inquirer": "^3.0.6",
    "source-map-support": "^0.4.14",
    "villa": "^0.2.11"
  }
}
