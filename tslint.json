{
  "extends": "vts",
  "rules": {
    "no-bitwise": false,
    "prefer-template": false,
    "prefer-const": false,
    "no-require-imports": false,
    "no-var-requires": false,
    "trailing-comma": [ false ],
    "return-undefined": false,
    "no-unbound-method": false,
    "variable-name": [
      true,
      "ban-keywords",
      "allow-pascal-case",
      "allow-trailing-underscore",
      "allow-leading-underscore"
    ]
  }
}
